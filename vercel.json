{
  "cleanUrls": true,
  "github": { "autoJobCancelation": true, "silent": true },
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ],
  "rewrites": [{ "source": "/([^.]+)", "destination": "/index" }]
}
